        <script><![CDATA[gs.print(lecture_groupe('Database',false));
//
// lecture groupe
function lecture_groupe(nom_groupe,trouve){
	
	if (!trouve){
		if (gs.getUser().isMemberOf(nom_groupe)){
			trouve=true;
		}else{
			var gr=new GlideRecord("sys_user_group");
			gr.addQuery("parent.name","nom_groupe");
			gr.query();
			while (gr.next() && !trouve){
				trouve=lecture_groupe(gr.name+'',trouve);
			}
		}
    }
	gs.print("nom groupe: "+ trouve);
	return trouve;
}	]]></script>
